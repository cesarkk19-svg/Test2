local SecureLoad = function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/cesarkk19-svg/Test2/refs/heads/main/Bd"))()
end

local Finity = SecureLoad()
local FinityWindow = Finity.new(true)
FinityWindow.ChangeToggleKey(Enum.KeyCode.Insert)

local function SafeFire(remote, ...)
    pcall(function()
        remote:FireServer(...)
    end)
end

local Rage = FinityWindow:Category("ABD")
local Sas = Rage:Sector("Main Functions")
local is = Rage:Sector("Secondary Functions")

local function DamagePlayer(target, params)
    local args = {
        target.Character.Humanoid,
        target.Character.HumanoidRootPart.CFrame,
        params.power or 100,
        params.rage or 30,
        params.force or Vector3.new(500, 500, 500),
        "rbxassetid://260430079",
        333,
        Color3.new(1, 1, 1),
        params.sound or "rbxassetid://6965860761",
        1,
        61212312323123
    }
    SafeFire(game:GetService("ReplicatedStorage").Damage, unpack(args))
end

Sas:Cheat("Button", "Lag Server", function()
    while task.wait() do
        SafeFire(game:GetService("ReplicatedStorage").PlayerStrongPunch, Vector3.new(9, 9, 9), Vector3.new(31, 21, 21), BrickColor.random())
    end
end)

Sas:Cheat("Button", "Kill All", function()
    for _, target in pairs(game:GetService("Players"):GetPlayers()) do
        if target ~= game.Players.LocalPlayer then
            DamagePlayer(target, {power = 100, rage = 33, force = Vector3.new(500, 500, 500)})
        end
    end
end)

Sas:Cheat("Button", "Invisible Stand", function()
    for _, part in pairs(game:GetService("Workspace").Entities:GetDescendants()) do
        if part:IsA("BasePart") then
            SafeFire(game:GetService("ReplicatedStorage").Transparency, part, 1)
        end
    end
end)

Sas:Cheat("Button", "Stun All", function()
    for _, target in pairs(game:GetService("Players"):GetPlayers()) do
        if target ~= game.Players.LocalPlayer then
            SafeFire(game:GetService("ReplicatedStorage").Knock, target.Character.Humanoid)
        end
    end
end)

local godModes = {Doppio = false, Enderman = false}

Sas:Cheat("Toggle", "God Mode 1", function(state)
    godModes.Doppio = state
    while godModes.Doppio do
        SafeFire(game:GetService("ReplicatedStorage").DoppioEpitaph)
        task.wait()
    end
end)

Sas:Cheat("Toggle", "God Mode 2", function(state)
    godModes.Enderman = state
    while godModes.Enderman do
        SafeFire(game:GetService("ReplicatedStorage").EndermanEpitaph)
        task.wait()
    end
end)

local function CollectItem(itemName)
    for _, item in pairs(game:GetService("Workspace").ItFolder:GetDescendants()) do
        if item.Name == itemName and item:IsA("Tool") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = item.Handle.CFrame
            task.wait(0.5)
        end
    end
end

is:Cheat("Button", "Collect Fruit", function()
    CollectItem("Rokakaka Fruit")
end)

is:Cheat("Button", "Collect Arrow", function()
    CollectItem("Arrow")
end)

local function SafeAnchor(part, state)
    pcall(function()
        part.Anchored = state
    end)
end

is:Cheat("Button", "Anti-Freeze", function()
    while task.wait(0.1) do
        local char = game.Players.LocalPlayer.Character
        SafeAnchor(char.HumanoidRootPart, false)
        SafeAnchor(char.Head, false)
    end
end)

is:Cheat("Button", "Rejoin Server", function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
end)

local loopFunctions = {PushAll = false, DamageLoop = false}

Sas:Cheat("Toggle", "Push Loop", function(state)
    loopFunctions.PushAll = state
    while loopFunctions.PushAll do
        for _, target in pairs(game:GetService("Players"):GetPlayers()) do
            if target ~= game.Players.LocalPlayer then
                SafeFire(game:GetService("ReplicatedStorage").CombatRemote, "Push", target.Character.Head, 1231123123123212312330.5)
            end
        end
        task.wait(0.1)
    end
end)

local originalFunctions = {FireServer = getmetatable(game:GetService("ReplicatedStorage")).__index.FireServer}

game:GetService("Players").LocalPlayer.CharacterRemoving:Connect(function()
    for _, loop in pairs(loopFunctions) do
        loop = false
    end
    for _, godMode in pairs(godModes) do
        godMode = false
    end
end)